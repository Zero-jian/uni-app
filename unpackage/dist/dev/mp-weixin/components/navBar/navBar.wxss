.navBar .navBar-fixed {
  background: #f07373;
  position: fixed;
  top: 0px;
  left: 0px;
  right: 0px;
}
.navBar .navBar-fixed .navBar-content {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 0 15px;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.navBar .navBar-fixed .navBar-content .navBar-search {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 0 15px;
  background: #ffffff;
  -webkit-border-radius: 20px;
          border-radius: 20px;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.navBar .navBar-fixed .navBar-content .navBar-search .navBar-search-icon {
  margin-right: 5px;
}
.navBar .navBar-fixed .navBar-content.navBar-isSearch {
  padding-left: 0;
}
.navBar .navBar-fixed .navBar-content.navBar-isSearch .navBar-search {
  -webkit-border-radius: 5px;
          border-radius: 5px;
}
.navBar .navBar-fixed .navBar-content .navBar-search-back {
  padding: 0 5px;
}

